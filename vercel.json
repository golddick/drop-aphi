{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "functions": {
    "app/**/*.js": {
      "includeFiles": [
        "lib/generated/prisma/**",
        "node_modules/.prisma/client/**",
        "node_modules/@prisma/engines/**"
      ]
    }
  },
  "env": {
    "PRISMA_GENERATE": "true"
  }
}
